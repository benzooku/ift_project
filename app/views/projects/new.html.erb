               <%= link_to "Proxima Plan", "", class:"self-center text-xl font-semibold whitespace-nowrap dark:text-white"%>

            <div class="flex items-center lg:order-2">              
                <%= link_to "Home", landing_path, class:"text-white bg-primary-700 hover:bg-primary-800 focus:ring-4 focus:ring-primary-300 font-medium rounded-lg text-sm px-4 lg:px-5 py-2 lg:py-2.5 mr-2 dark:bg-primary-600 dark:hover:bg-primary-700 focus:outline-none dark:focus:ring-primary-800"%>
