<%= form_with model: @project, url: projects_url, method: :post do |form| %>
  <%= form.text_field :name, placeholder: "Poroject name", class:"rounded mx-4 mt-4" %> <br>
  <%= form.submit 'Create Project', class:"bg-violet-700 hover:bg-violet-800 text-white font-bold py-2 px-4 rounded mx-4 mt-2" %>
<% end %>
